{
        "HelloController": [
            {
                "path": "/hello",
                "controller": "ExampleController@hello"
            }
        ],
        "WorldController": [
            {
                "path": "/world/me",
                "controller": "ExampleController@world"
            }
        ],
        "IndexController": [
            {
                "path": "/",
                "controller": "ExampleController@index"
            }
        ],
        "SlugController": [
            {
                "path" : "/test/^slug",
                "controller" : "ExampleController@slug"
            }
        ]
}